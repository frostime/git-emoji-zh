{
	"name": "@frostime/git-emoji",
	"displayName": "Git Emoji Commit - 个人定制版",
	"description": "提交 git 的时候，加上 emoji 前缀！",
	"version": "1.2.0",
	"publisher": "frostime",
	"author": {
		"name": "maixiaojie"
	},
	"license": "MIT",
	"homepage": "https://github.com/frostime/git-emoji-zh/blob/master/README.md",
	"bugs": {
		"url": "https://github.com/frostime/git-emoji-zh/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/frostime/git-emoji-zh.git"
	},
	"engines": {
		"vscode": "^1.85.0"
	},
	"categories": [
		"Other"
	],
	"icon": "images/logo.png",
	"preview": false,
	"galleryBanner": {
		"color": "#f0efe7",
		"theme": "light"
	},
	"keywords": [
		"git",
		"emoji"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.gitEmoji",
				"title": "git emoji commit message",
				"icon": {
					"dark": "images/git-emoji.svg",
					"light": "images/git-emoji_light.svg"
				}
			},
			{
				"command": "extension.switching",
				"title": "git emoji label switching"
			}
		],
		"menus": {
			"scm/title": [
				{
					"when": "scmProvider == git",
					"command": "extension.gitEmoji",
					"group": "navigation"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "pnpm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "pnpm run compile",
		"test": "node ./out/test/runTest.js",
		"patch": "vsce publish patch",
		"minor": "vsce publish minor"
	},
	"devDependencies": {
		"@types/glob": "^7.2.0",
		"@types/mocha": "^5.2.7",
		"@types/node": "^10.17.60",
		"@types/vscode": "^1.85.0",
		"@vscode/test-electron": "^2.3.8",
		"glob": "^7.2.3",
		"mocha": "^6.2.3",
		"tslint": "^5.20.1",
		"typescript": "^5.3.3"
	}
}
